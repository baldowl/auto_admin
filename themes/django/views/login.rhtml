<%
@body_class = ''
@content_class = 'colM'
@title = 'Log in'
%>
<% form_tag do %>
<p class="aligned">
<label for="username">Username:</label> <%= text_field_tag 'username', '' %>
</p>
<p class="aligned">
<label for="password">Password:</label> <%= password_field_tag 'password', '' %>
</p>

<div class="aligned ">
<label>&nbsp;</label><%= submit_tag 'Log in' %>
</div>

<script type="text/javascript">
document.getElementById('username').focus()
</script>
<% end %>
